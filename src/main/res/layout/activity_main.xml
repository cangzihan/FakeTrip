<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2BA567"
    android:gravity="center|top"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="135dp"
                android:layout_gravity="center"
                app:srcCompat="@mipmap/g2"
                tools:layout_editor_absoluteY="16dp" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="-18dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/view"
                    android:layout_width="313dp"
                    android:layout_height="330dp"
                    android:background="#FFFFFF"
                    android:layout_gravity="top|center"
                    app:shapeAppearance="@style/RoundedStyle" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="top|center"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView9"
                        android:layout_width="313dp"
                        android:layout_height="33dp"
                        android:layout_marginTop="-1dp"
                        app:srcCompat="@mipmap/withtext2x" />

                    <EditText
                        android:id="@+id/textView6"
                        android:layout_width="190dp"
                        android:layout_height="32dp"
                        android:background="#FFFFFF"
                        android:hint="150***3841的动态行程卡"
                        android:text="150***3841的动态行程卡"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:text="更新于:"
                            android:textColor="#B1B1B1"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/TextTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:layout_marginStart="8dp"
                            android:text="2022.12.11 12:02:41"
                            android:textColor="#B1B1B1"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <pl.droidsonroids.gif.GifImageView
                        android:id="@+id/activity_gif_giv"
                        android:layout_width="190dp"
                        android:layout_height="180dp"
                        android:src="@mipmap/gif_green" />

                    <View
                        android:id="@+id/view2"
                        android:layout_width="290dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="18dp"
                        android:background="#B0B0B0"
                        />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:layout_marginStart="8dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="您于前7天内到达或途径: "
                            android:textColor="#B1B1B1"
                            android:textSize="12sp" />

                        <EditText
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:hint="北京市"
                            android:text="北京市"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="12dp"
                android:text="结果包含您在前7天内到访的国家（地区）与停留4小时以上的国内城市"
                android:textColor="#FEFEFE"
                android:textSize="10.5sp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="色卡仅对到访地作提醒，不关联健康状况"
                android:textColor="#FEFEFE"
                android:textSize="10.5sp"
                android:layout_gravity="center"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="18dp"
                android:layout_marginBottom="12dp"
                android:orientation="horizontal">

                <View
                    android:id="@+id/view3"
                    android:layout_width="95dp"
                    android:layout_height="1dp"
                    android:layout_marginEnd="20dp"
                    android:layout_gravity="center"
                    android:background="#FEFEFE" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="本服务联合提供"
                    android:layout_gravity="center"
                    android:textColor="#FEFEFE"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/view4"
                    android:layout_width="95dp"
                    android:layout_height="1dp"
                    android:layout_marginStart="20dp"
                    android:layout_gravity="center"
                    android:background="#FEFEFE" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="6dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="90dp"
                    android:layout_height="24dp"
                    app:srcCompat="@mipmap/xty0" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="77dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="-6dp"
                    app:srcCompat="@mipmap/dianxin0" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="69dp"
                    android:layout_height="26dp"
                    android:layout_marginStart="-6dp"
                    app:srcCompat="@mipmap/yidong0" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="60dp"
                    android:layout_height="22dp"
                    app:srcCompat="@mipmap/liant0" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="72dp"
                    android:layout_height="22dp"
                    app:srcCompat="@mipmap/guangdian0" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_gravity="center"
                android:text="客服热线: 10000 / 10086 / 10010 / 10099"
                android:textColor="#FEFEFE"
                android:textSize="10.5sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="79dp"
                    android:layout_height="15dp"
                    android:layout_marginBottom="12dp"
                    app:srcCompat="@mipmap/ucload" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="1dp"
                    android:text="提供云服务技术支持"
                    android:textColor="#FEFEFE"
                    android:textSize="10.5sp" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>


</LinearLayout>